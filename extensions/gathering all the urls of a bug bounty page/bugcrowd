javascript: (() => { function e(e, t) { var r = document.createElement("a"); r.setAttribute("href", "data:text/plain;charset=utf-8," + encodeURIComponent(t)), r.setAttribute("download", e), r.style.display = "none", document.body.appendChild(r), r.click(), document.body.removeChild(r) } result = "", document.querySelectorAll(".cc-rewards-link-table tr td:first-of-type").forEach(function (e) { console.log(e.innerText), result += e.innerText.trim() + "\n" }), e(filename = `${document.querySelector("h1").textContent.trim()}.txt`, result), (newCopyInput = document.createElement("input")).type = "text", newCopyInput.value = filename, document.querySelector("body").appendChild(newCopyInput), copie = newCopyInput.value, newCopyInput.remove(), navigator.clipboard.writeText(copie) })();
