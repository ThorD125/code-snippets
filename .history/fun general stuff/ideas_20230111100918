file explorer
function on_click_of_item {
    if (item is file){
        preview file
    }
    else  if (item is folder){
        return folder structure
    } else if (item is link) {
        return linked item or folder
    } 
    esle {
        error in file
    }
}
function on load of folder {
    return folder structure
    + add eventlistener to folder structure
}
