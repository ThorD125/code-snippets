file explorer
on_click_of_item {
    if (item is file){
        preview_file
    }
    else  if (item is folder){
        return folder_structure
    } else if (item is link) {
        return preview_linked_item_or_folder
    } 
    esle {
        error_in_file
    }
}
on_load_of_folder {
    return folder structure
    + on_click_of_item
}
